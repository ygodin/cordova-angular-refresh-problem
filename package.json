{
    "name": "helloworld",
    "displayName": "HelloCordova",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "cordova": "cordova run ios --simulator",
        "web": "npx lite-server --baseDir=\"./www/\""
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-ios": "^5.1.1",
        "cordova-plugin-ionic-webview": "^4.2.1",
        "cordova-plugin-whitelist": "^1.3.4",
        "cordova-plugin-wkwebview-file-xhr": "git+https://github.com/ygodin/cordova-plugin-wkwebview-file-xhr.git#master"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-wkwebview-file-xhr": {},
            "cordova-plugin-ionic-webview": {}
        },
        "platforms": [
            "ios"
        ]
    }
}
